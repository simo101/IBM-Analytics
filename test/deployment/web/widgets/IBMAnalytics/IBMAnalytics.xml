<widget id="IBMAnalytics.widget.IBMAnalytics" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>IBM Analytics</name>
    <description>This widget allows you to track web traffic using the IBM analytics platform.</description>
    <icon>
    
    </icon>
    <properties>
        <property key="applicationName" type="string" required="true" defaultValue="">
            <caption>App Name</caption>
            <category>Setup</category>
            <description></description>
        </property>
        <property key="apiKey" type="string" required="true" defaultValue="">
            <caption>API Key</caption>
            <category>Setup</category>
            <description></description>
        </property>
        <property key="instanceID" type="string" required="true" defaultValue="">
            <caption>Instance ID</caption>
            <category>Setup</category>
            <description></description>
        </property>
        <property key="bmsRegion" type="enumeration" defaultValue="REGION_US_SOUTH">
            <caption>BMS Region</caption>
            <category>Setup</category>
            <description></description>
            <enumerationValues>
                 <enumerationValue key="REGION_US_SOUTH">REGION_US_SOUTH</enumerationValue>
                 <enumerationValue key="REGION_UK">REGION_UK</enumerationValue>
                 <enumerationValue key="REGION_SYDNEY">REGION_SYDNEY</enumerationValue>
            </enumerationValues>
        </property>
        <property key="bmsDevice" type="enumeration" defaultValue="ALL">
            <caption>BMS Device</caption>
            <category>Setup</category>
            <description></description>
            <enumerationValues>
                 <enumerationValue key="ALL">ALL</enumerationValue>
                 <enumerationValue key="LIFECYCLE">LIFECYCLE</enumerationValue>
                 <enumerationValue key="NETWORK">NETWORK</enumerationValue>
            </enumerationValues>
        </property>
        <property key="hasUsrCont" type="boolean" defaultValue="true">
            <caption>Has User Context</caption>
            <category>Setup</category>
            <description></description>
        </property>
        <property key="storeLogs" type="boolean" defaultValue="true">
            <caption>Store Logs</caption>
            <category>Setup</category>
            <description></description>
        </property>
        <property key="enableUsageAnalytics" type="boolean" defaultValue="true">
            <caption>Usage Analytics</caption>
            <category>Setup</category>
            <description></description>
        </property>
    </properties>
</widget>
