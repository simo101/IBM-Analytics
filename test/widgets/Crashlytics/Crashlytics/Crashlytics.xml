<?xml version="1.0" encoding="utf-8" ?>
<widget id="Crashlytics.widget.Crashlytics" needsEntityContext="false" mobile="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Crashlytics</name>
    <description>Crashlytics (fabric.io) integration.</description>

    <icon></icon>

    <properties>
        <property key="skip" type="string" required="false" defaultValue="">
            <caption>Skip messages</caption>
            <category>Behavior</category>
            <description>Skip error messages containing these texts. Multiple texts can be separated by a "|". The comparison is not case sensitive.</description>
        </property>
    </properties>
</widget>
